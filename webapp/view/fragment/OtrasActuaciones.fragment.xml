<!-- INI HADA 3 -->
<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:layout="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    xmlns:t="sap.ui.table" 
    xmlns:d="sap.ui.unified"
>
    <Dialog
        id="dialogOtrasAct"
        class="sapUiPopupWithPadding"
        contentWidth="750px"
        contentHeight="520px"
        busyIndicatorDelay="0"  
        showHeader="true" >

        <t:Table id="tableAct" selectionMode="None" rows="{otrasAct>/}"  ariaLabelledBy="title" rowHeight="auto" visibleRowCount="13">
            <t:columns>
                <t:Column id="Act" width="11rem">
                    <Label  text="Actuación" />
                        <t:template>
                            <Text tooltip="{otrasAct>CodAct}" text="{otrasAct>CodAct}" wrapping="false" />
                        </t:template>
                </t:Column>
                <t:Column id="desAct" width="33rem" >
                    <Label  text="Descripción Actuación" />
                        <t:template>
                            <Text  tooltip="{otrasAct>DescAct}" text="{otrasAct>DescAct}" wrapping="false" />
                        </t:template>
                </t:Column>             
                </t:columns>
        </t:Table>	
		<endButton>
			<Button id="Cancelar" text="Cancelar" press="onCloseOtrasAct"/>
		</endButton>
    </Dialog>
</core:FragmentDefinition>
<!-- FIN HADA 3 -->
